  "author": "Alessandro Casazza",
  "license": "MIT",
  "dependencies": {
    "babel-jest": "^6.0.1",
    "bootstrap": "^3.3.5",
    "eonasdan-bootstrap-datetimepicker": "^4.15.35",
    "jquery": "^2.1.4",
    "moment": "^2.10.6",
    "react-addons-perf": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-credit-card": "^0.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "css-loader": "^0.23.0",
    "eslint": "^1.5.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.9.0",
    "file-loader": "^0.8.4",
    "jest-cli": "^0.8.0",
    "json-loader": "^0.5.3",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "node-libs-browser": "^0.5.3",
    "react-addons-test-utils": "^0.14.3",
